#You are given a string and your task is to swap cases. In other words, convert all lowercase letters to uppercase letters and vice versa.
def swap_case(s):
    d=""
    for i in s:
        if i.isupper()==True:
            d+=i.lower()
        elif i.islower()==True:
            d+=i.upper()
        else:
            d+=i
    return d
if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
